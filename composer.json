{
    "name": "swew/test",
    "description": "A framework that is designed to fix the fatal flaw of other frameworks.",
    "license": "MIT",
    "authors": [
        {
            "name": "AVIL13",
            "email": "avil@swew.app"
        }
    ],
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "SWEW\\Test\\": "src/"
        },
        "files": [
            "src/helpers.php",
            "src/dd.php"
        ],
        "exclude-from-classmap": [
            "spec",
            "/Tests/",
            "/test/",
            "/tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "laravel/pint": "^0.2.0",
        "phpstan/phpstan": "^1.7",
        "vimeo/psalm": "^4.23"
    },
    "scripts": {
        "test": "php ./tests/Suite/RunTest.php",
        "static": "./vendor/bin/psalm",
        "stan": "vendor/bin/phpstan analyse src",
        "code-check": "./vendor/bin/pint",
        "check": [
            "@static",
            "@stan",
            "@code-check"
        ]
    }
}
